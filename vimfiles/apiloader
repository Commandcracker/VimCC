local apiPath = "/utils/vimfiles/"
local apis = {
	"config",
	"global",
	"command",
	"screen",
	"vimode",
	"file",
	"logger",
}
function load()
	for i=1, #apis do
		local test = os.loadAPI(apiPath..apis[i])
		if not test then
			error(apis[i])
		end
	end
end

function unload()
	for i=1, #apis do
		os.unloadAPI(apiPath..apis[i])
	end
	os.unloadAPI("/utils/vimfiles/apiloader")
end
