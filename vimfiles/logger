local filename
local file
logLevel = {
	ALL = "ALL",
	WARNINGS = "WARNINGS",
}
local level

function init( path, llevel )
	filename = path
	level = llevel
	file = fs.open(filename, "a")
end

function info( message )
	if level == "WARNINGS" then else
		file.writeLine("INFO:"..message)
		file.flush()
	end
end

function warning( message )
	file.writeLine("WARN:"..message)
	file.flush()
end
